{
  "name": "panopla/laravel-translatable",
  "description": "A laravel package to support translatable eloquent models.",
  "keywords": [
    "laravel",
    "translation",
    "laravel 5"
  ],
  "require": {
    "php": "^5.5",
    "illuminate/support": "~5.0",
    "illuminate/database": "~5.0",
    "illuminate/console": "~5.0"
  },
  "require-dev": {
    "phpspec/phpspec": "^2.3",
    "phpunit/phpunit": "4.8.*"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "panopla\\Translatable\\": "src/Translatable/"
    }
  },
